<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D04DCD54"
    tools:context=".MainActivity5">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="0"
        android:text="Card"
        android:textColor="@color/white"
        android:textSize="35sp"
        android:textStyle="bold"
        android:layout_marginLeft="20dp"/>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="0"
        android:text="Simple and easy to use app"
        android:textColor="@color/white"
        android:textSize="19.5sp"
        android:textStyle="bold"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="20dp"/>

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="165dp"
        android:layout_height="110dp"
        android:layout_row="2"
        android:layout_column="0"
        android:layout_marginLeft="20dp"
        android:background="@drawable/ovalo"
        app:srcCompat="@drawable/biblia" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="0"
        android:text="Text"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="85dp"
        android:textColor="@color/black"/>

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="165dp"
        android:layout_height="110dp"
        android:layout_row="2"
        android:layout_column="1"
        android:layout_marginLeft="-50dp"
        android:background="@drawable/ovalo"
        app:srcCompat="@drawable/iglesia" />
    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="1"
        android:text="Adress"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="90dp"
        android:textColor="@color/black"/>


    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="165dp"
        android:layout_height="110dp"
        android:layout_row="3"
        android:layout_column="0"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="26dp"
        android:background="@drawable/ovalo"
        app:srcCompat="@drawable/assasin" />
    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="0"
        android:text="Character"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="110dp"
        android:textColor="@color/black"/>


    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="165dp"
        android:layout_height="110dp"
        android:layout_row="3"
        android:layout_column="1"
        android:layout_marginLeft="-50dp"
        android:layout_marginTop="26dp"
        android:background="@drawable/ovalo"
        app:srcCompat="@drawable/credit" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="1"
        android:text="Bank card"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="105dp"
        android:textColor="@color/black"/>

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="165dp"
        android:layout_height="110dp"
        android:layout_row="4"
        android:layout_column="0"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="26dp"
        android:background="@drawable/ovalo"
        app:srcCompat="@drawable/key" />
    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="4"
        android:layout_column="0"
        android:text="Password"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="110dp"
        android:textColor="@color/black"/>


    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="165dp"
        android:layout_height="110dp"
        android:layout_row="4"
        android:layout_column="1"
        android:layout_marginLeft="-50dp"
        android:layout_marginTop="26dp"
        android:background="@drawable/ovalo"
        app:srcCompat="@drawable/logistics" />
    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="4"
        android:layout_column="1"
        android:text="Logistics"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="108dp"
        android:textColor="@color/black"/>

    <LinearLayout
        android:layout_width="355dp"
        android:layout_height="55dp"
        android:layout_marginLeft="-355dp"
        android:layout_marginTop="155dp"
        android:background="@drawable/ovalo"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="5"
            android:layout_column="0"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:background="#00FFFFFF"
            app:srcCompat="@drawable/herramienta" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:text="Settings"
            android:textColor="@color/black"
            android:textSize="20sp" />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="3"
        app:srcCompat="@drawable/perfiltwo"
        android:layout_marginTop="5dp"/>


</GridLayout>